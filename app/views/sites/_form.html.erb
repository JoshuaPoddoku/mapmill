<%= form_for :site, url: sites_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :date %><br>
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description, :rows => 6 %>
  </p>
   <p>
    <%= f.submit 'Begin' , :class => 'btn btn-primary' %>
  </p>

<% end %>
