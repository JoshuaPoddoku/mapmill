<%= form_for :site,:remote => true, :authenticity_token => true, url: sites_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :date %><br>
    <%= f.text_field :date %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
   <p>
    <%= f.submit %>
  </p>

<% end %>
